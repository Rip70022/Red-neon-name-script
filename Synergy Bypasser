        MinimizeKey = Enum.KeyCode.LeftControl 
    })

    local Tabs = {
        BypassChat = Window:AddTab({ Title = "Bypass‚Ä¢Chat", Icon = "code" }),
        Discord = Window:AddTab({ Title = "Discord‚Ä¢Server", Icon = "info" })
    }

