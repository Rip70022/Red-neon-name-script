-- Crear una funci√≥n de notificaci√≥n
local function createNotification(title, text)
    local notification = Instance.new("Frame")
    notification.Size = UDim2.new(0, 300, 0, 100)
    notification.Position = UDim2.new(0.5, -150, 0, 50)
    notification.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    notification.BorderSizePixel = 0
    notification.Parent = game:GetService("CoreGui")

    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, 0, 0, 40)
    titleLabel.BackgroundTransparency = 1
    titleLabel.TextColor3 = Color3.fromRGB(240, 240, 240)
    titleLabel.Text = title
    titleLabel.TextScaled = true
    titleLabel.Parent = notification

    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 1, -40)
    textLabel.Position = UDim2.new(0, 0, 0, 40)
    textLabel.BackgroundTransparency = 1
    textLabel.TextColor3 = Color3.fromRGB(240, 240, 240)
    textLabel.Text = text
    textLabel.TextScaled = true
    textLabel.Parent = notification

    local tweenIn = game:GetService("TweenService"):Create(notification, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -150, 0, 50)})
    local tweenOut = game:GetService("TweenService"):Create(notification, TweenInfo.new(0.5), {Position = UDim2.new(0.5, -150, 0, -100)})

    tweenIn:Play()
    tweenIn.Completed:Wait()

    wait(NotificationDuration)

    tweenOut:Play()
    tweenOut.Completed:Wait()

    notification:Destroy()
end

local function sendToChat(message)
    -- Enviar el mensaje al chat utilizando el evento Remote
    local success, err = pcall(function()
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local chatEvent = ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
        chatEvent:FireServer(message, Enum.ChatColor.White)
    end)

    if not success then
        createNotification("Error", "No se pudo enviar el mensaje: " .. tostring(err))
    end
end

local function executeHub()
    print("Executing the hub...")  -- Debug line

    -- Try loading Fluent
    print("Loading Fluent...")  -- Debug line
    local success, Fluent = pcall(function()
        return loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
    end)
    if not success then
        createNotification("Error", "Failed to load Fluent module.")
        print("Error loading Fluent:", Fluent)  -- Debug line
        return
    end
    print("Fluent loaded successfully")  -- Debug line

    -- Try loading SaveManager
    print("Loading SaveManager...")  -- Debug line
    local successSaveManager, SaveManager = pcall(function()
        return loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
    end)
    if not successSaveManager then
        createNotification("Error", "Failed to load SaveManager module.")
        print("Error loading SaveManager:", SaveManager)  -- Debug line
        return
    end
    print("SaveManager loaded successfully")  -- Debug line

    -- Try loading InterfaceManager
    print("Loading InterfaceManager...")  -- Debug line
    local successInterfaceManager, InterfaceManager = pcall(function()
        return loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
    end)
    if not successInterfaceManager then
        createNotification("Error", "Failed to load InterfaceManager module.")
        print("Error loading InterfaceManager:", InterfaceManager)  -- Debug line
        return
    end
    print("InterfaceManager loaded successfully")  -- Debug line

    -- Create the hub window
    print("Creating hub window...")  -- Debug line
    local Window = Fluent:CreateWindow({
        Title = "üî•Synergy Bypasserüî•",
        SubTitle = "Ô∏é‚Ä¢‚Ä¢‚Ä¢‚Ä¢Ô∏é¬§„Ää üí†By ERROR_USERBANNEDüí† „Äã",
        TabWidth = 160,
        Size = UDim2.fromOffset(460, 250),
        Acrylic = true,
        Theme = "Dark",
        MinimizeKey = Enum.KeyCode.LeftControl 
    })

    local Tabs = {
        BypassChat = Window:AddTab({ Title = "Bypass‚Ä¢Chat", Icon = "code" }),
        Discord = Window:AddTab({ Title = "Discord‚Ä¢Server", Icon = "info" })
    }

    local Options = Fluent.Options

    do
        Fluent:Notify({
            Title = "Loaded",
            Content = "üî•Synergy Bypasserüî•",
            SubContent = "„Ää üåÄBy ERROR_USERBANNED üåÄ „Äã",
            Duration = 10
        })
        
        -- A√±adir botones para cada notificaci√≥n
        for _, notification in ipairs(notifications) do
            Tabs.BypassChat:AddButton({
                Title = notification.title,
                Description = "Click to send to chat",
                Callback = function()
                    sendToChat(notification.content)
                    createNotification("Sent", "Sent to chat: " .. notification.content)
                end
            })
        end
        
        Tabs.Discord:AddButton({
            Title = "‚ô¶Ô∏èJoin Discord Server‚ô¶Ô∏è",
            Description = "üåÄClick to copy the Discord invite linküåÄ.",
            Callback = function()
                setclipboard("https://discord.gg/7ThdVFRs")
                Fluent:Notify({
                    Title = "Copied",
                    Content = "Discord invite link copied to clipboard",
                    Duration = 3
                })
            end
        })
    end
end

-- Crear una GUI
local function createGUI()
    local playerGui = game.Players.LocalPlayer:WaitForChild("PlayerGui")

    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "ScreenGui"
    screenGui.Parent = playerGui

    local background = Instance.new("Frame")
    background.Size = UDim2.new(0, 300, 0, 200)
    background.Position = UDim2.new(0.5, -150, 0, 50)
    background.BackgroundTransparency = 0.5
    background.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    background.Parent = screenGui

    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 40)
    title.BackgroundTransparency = 1
    title.TextColor3 = Color3.fromRGB(240, 240, 240)
    title.Text = "üåÄSynergy BypasserüåÄ"
    title.TextScaled = true
    title.Parent = background

    local textBox = Instance.new("TextBox")
    textBox.Name = "TextBox"
    textBox.Size = UDim2.new(1, 0, 0, 40)
    textBox.Position = UDim2.new(0, 0, 0, 40)
    textBox.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    textBox.TextColor3 = Color3.fromRGB(240, 240, 240)
    textBox.PlaceholderText = "Enter key here"
    textBox.TextScaled = true
    textBox.Parent = background

    local buttonFrame = Instance.new("Frame")
    buttonFrame.Size = UDim2.new(1, 0, 0, 40)
    buttonFrame.Position = UDim2.new(0, 0, 0, 100)
    buttonFrame.BackgroundTransparency = 1
    buttonFrame.Parent = background

    local enterButton = Instance.new("TextButton")
    enterButton.Size = UDim2.new(0.5, 0, 1, 0)
    enterButton.Position = UDim2.new(0, 0, 0, 0)
    enterButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
    enterButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    enterButton.Text = "Enter"
    enterButton.Parent = buttonFrame

    local exitButton = Instance.new("TextButton")
    exitButton.Size = UDim2.new(0.5, 0, 1, 0)
    exitButton.Position = UDim2.new(0.5, 0, 0, 0)
    exitButton.BackgroundColor3 = Color3.fromRGB(170, 0, 0)
    exitButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    exitButton.Text = "Exit"
    exitButton.Parent = buttonFrame

    -- Evento de presionar el bot√≥n de entrada
    enterButton.MouseButton1Click:Connect(function()
        if textBox.Text == Key then
            KeyEntered = true
            createNotification("Acceso Concedido", "Acceso al Hub permitido.")
            background.Visible = false
            executeHub()
        else
            createNotification("Acceso Denegado", "La clave ingresada es incorrecta.")
        end
    end)

    -- Evento de presionar el bot√≥n de salida
    exitButton.MouseButton1Click:Connect(function()
        background:Destroy()
    end)
end

-- Llama a crear la GUI
createGUI()
