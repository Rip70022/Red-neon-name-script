    local function Bypass(input)
        local lowered = input:lower();
        local output = "";
        for i = 1, #lowered do
            local char = lowered:sub(i, i);
            output = output .. (characterstable[char] or char);
        end
        return output
    end

    -- Function to send the bypassed message to chat
