    Title = "♦️Join Discord Server♦️",
    Description = "🌀Click to copy the Discord invite link🌀.",
    Callback = function()
        setclipboard("https://discord.gg/7ThdVFRs")
        Fluent:Notify({
            Title = "Copied",
            Content = "Discord invite link copied to clipboard",
            Duration = 3
        })
    end
})


