    local characterstable = {
      ["q"] = "<q>", ["w"] = "<w>", ["e"] = "<e>", ["r"] = "<r>", ["t"] = "<t>",
    ["y"] = "<y>", ["u"] = "<u>", ["i"] = "<i>", ["o"] = "<o>", ["p"] = "<p>",
    ["a"] = "<a>", ["s"] = "<s>", ["d"] = "<d>", ["f"] = "<f>", ["g"] = "><g>",
